<%- include('./base/header.ejs') %>
<div class="container">
    <div class="row">
        <h4 class="text-center my-3">Student List</h4>
        <table class = "table text-center">
            <thead>
                <tr>
                    <th rollNo></th>
                    <th firstName></th>
                    <th lastName></th>
                    <th fatherName></th>
                    <th adharCardNo></th>
                    <th mobileNo></th>

                </tr>
            </thead>
            <% students.forEach((student)=>{ %>

                <tr>
                    <td><%= student.rollNo %></td>
                    <td><%= student.firstName %></td>
                    <td><%= student.lastName %></td>
                    <td><%= student.fatherName %></td>
                    <td><%= student.adharCardNo %></td>
                    <td><%= student.mobileNo %></td>
                </tr>
            <% }) %>

        </table>

    </div>
</div>
<%- include('./base/footer.ejs') %>